{
  "name": "my-medusa-store",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "start":"npx medusa start",
    "build": "medusa build && ln -s .medusa/server/public/ public",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "dev": "medusa develop",
    "sync-products": "ts-node ./src/scripts/sync-products.ts",
    "predeploy": "medusa db:migrate",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "2.7.0",
    "@medusajs/cli": "2.7.0",
    "@medusajs/framework": "2.7.0",
    "@medusajs/medusa": "2.7.0",
    "@medusajs/medusa-js": "^6.1.10",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@sgftech/payment-razorpay": "^2.1.11",
    "@tsc_tech/medusa-plugin-razorpay-payment": "^0.0.11",
    "@types/algoliasearch": "^3.34.11",
    "@types/multer": "^1.4.12",
    "@types/papaparse": "^5.3.16",
    "algoliasearch": "^5.27.0",
    "awilix": "^8.0.1",
    "dotenv": "^16.5.0",
    "ioredis": "^5.6.1",
    "multer": "^2.0.1",
    "next": "^15.3.3",
    "papaparse": "^5.5.3",
    "pg": "^8.13.0",
    "rate-limiter-flexible": "^7.1.1",
    "razorpay": "^2.9.6",
    "twilio": "^5.6.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.7.0",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  }
}
